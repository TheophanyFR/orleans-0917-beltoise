<span id="anchorPresentation" class="anchor"></span>
<section>
    <div class="section-content">
        <h2 class="presentation-title">Beltoise & Fils</h2>
        <div class="row">
            <div class="col-xs-10 col-xs-offset-1 presentation-text">
                <p>
                    {% for presentationAccueil in presentationAccueils %}
                        {{ presentationAccueil.texte }}
                    {% endfor %}
                </p>
            </div>
        </div>
        <div class="row-logo-certif">
            {% for logo in logos %}
                <div class="logo-certif">
                    <p><img src="assets/uploads/{{ logo.name }}" alt="Certification"></p>
                </div>
            {% endfor %}
        </div>
    </div>
</section>
